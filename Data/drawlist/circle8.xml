<?xml version="1.0" ?>
<drawlist name="circle8">
	<!-- 8 vertex circle -->
	<polygon>
		<loop name="angle" from="0" to="8" by="1">
			<vertex>
				<x>
					<sinewave period="8">
						<input>
							<angle/>
						</input>
					</sinewave>
				</x>
				<y>
					<sinewave phase="0.25" period="8">
						<input>
							<angle/>
						</input>
					</sinewave>
				</y>
			</vertex>
		</loop>
	</polygon>
</drawlist>
