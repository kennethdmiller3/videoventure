			<block length="3.0">
				<pushmatrix>
					<color>
						<key time="0.0" r="0.4" g="0.7" b="1.0" a="0.5"/>
						<key time="3.0" r="0.0" g="0.0" b="1.0" a="0.0"/>
					</color>
					<repeat count="6">
						<repeat count="12">
							<pushmatrix>
								<translate>
									<key time="0.0" x="0" y="0"/>
									<key time="3.0" x="1000" y="0"/>
								</translate>
								<scale>
									<key time="0.0" x="15.0" y="1.0"/>
									<key time="3.0" x="1.0" y="1.0"/>
								</scale>
								<calllist name="circle16"/>
							</pushmatrix>
							<rotate angle="30"/>
						</repeat>
						<scale x="0.8" y="0.8"/>
