<?xml version="1.0" ?>
<template name="playermachinegunbullet">
	<collidable>
		<body name="main">
			<fast value="1" />
			<circle radius="1">
				<friction value="1" />
				<restitution value="0" />
				<density value="0.2" />
				<filter name="bluebullet" />
			</circle>
			<fixedrotation value="1" />
		</body>
	</collidable>
	<renderable radius="16">
		<triangle_strip>
			<color r="0.7" g="0.4" b="1.0" a="1.0" />
			<vertex x="0" y="2" />
			<vertex x="0.5" y="0" />
			<vertex x="-0.5" y="0" />
			<vertex>
				<key time="0.000000" x="0.5" y="0" />
				<key time="0.033333" x="0" y="-16" />
				<key time="0.500000" x="0" y="-16" />
			</vertex>
			<vertex>
				<key time="0.000000" x="-0.5" y="0" />
				<key time="0.033333" x="0" y="-16" />
				<key time="0.500000" x="-0" y="-16" />
			</vertex>
		</triangle_strip>
	</renderable>
	<expire time="0.5" />
	<bullet damage="1" spawnondeath="playermachinegunhit" />
</template>
