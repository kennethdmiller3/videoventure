			<box w="8" h="8">
				<filter name="environment" />
			</box>
		</body>
	</collidable>
	<renderable radius="12">
		<drawlist>
			<color r="0.2" g="0.3" b="0.5" a="1.0" />
			<quads>
				<vertex x="-8" y="8" />
				<vertex x="8" y="8" />
				<vertex x="8" y="-8" />
				<vertex x="-8" y="-8" />
			</quads>
		</drawlist>
	</renderable>
</template>
