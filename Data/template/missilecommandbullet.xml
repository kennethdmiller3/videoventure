<?xml version="1.0" ?>
<template name="missilecommandbullet">
	<collidable>
		<body name="main">
			<fast value="1" />
			<circle radius="1">
				<friction value="1" />
				<restitution value="0" />
				<density value="1.0" />
				<category value="2" />
				<mask bit1="0" bit2="0" bit3="0" bit4="0" />
			</circle>
			<fixedrotation value="1" />
		</body>
	</collidable>
	<renderable radius="240">
		<triangle_strip>
			<drawlist>
				<color r="1.0" g="1.0" b="1.0" a="1.0" />
				<vertex x="0" y="4" />
				<vertex x="1.5" y="-0.5" />
				<vertex x="-1.5" y="-0.5" />
				<vertex x="0.0" y="-2" />
			</drawlist>
			<block length="0.1" repeat="5">
				<color>
					<key time="0.00" r="0.0" g="1.0" b="0.5" a="1.0" />
					<key time="0.05" r="0.0" g="0.5" b="1.0" a="1.0" />
					<key time="0.10" r="0.0" g="1.0" b="0.5" a="1.0" />
				</color>
			</block>
			<vertex x="0.5" y="-1" />
			<vertex x="-0.5" y="-1" />
			<vertex>
				<key time="0.000000" x="0.5" y="-1" />
				<key time="0.666667" x="0.5" y="-240" />
			</vertex>
			<vertex>
				<key time="0.000000" x="-0.5" y="-1" />
				<key time="0.666667" x="-0.5" y="-240" />
			</vertex>
		</triangle_strip>
		<pushmatrix>
			<set name="t" type="translate">
				<key time="0.000000" x="0" y="0" z="0" />
				<key time="0.666667" x="0" y="-240" z="0" />
			</set>
			<add name="t" type="translate" variable="localaim" />
			<translate variable="t" />
			<drawlist>
				<color r="0.1" g="0.6" b="1.0" a="1.0" />
				<rotate angle="45" />
				<quads>
					<vertex x="2.5" y="0.5" />
					<vertex x="-2.5" y="0.5" />
					<vertex x="-2.5" y="-0.5" />
					<vertex x="2.5" y="-0.5" />
					<vertex x="0.5" y="2.5" />
					<vertex x="-0.5" y="2.5" />
					<vertex x="-0.5" y="-2.5" />
					<vertex x="0.5" y="-2.5" />
				</quads>
			</drawlist>
			<clear name="t" />
		</pushmatrix>
	</renderable>
	<expire time="0.666667" switchonexpire="missilecommandexplosion" reticule="1" />
	<bullet damage="0" spawnondeath="missilecommandexplosion" />
</template>
