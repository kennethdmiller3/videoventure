<?xml version="1.0" ?>
<template name="playership">
	<collidable>
		<body name="main">
			<circle radius="8">
				<density value="1" />
				<friction value="0.5" />
				<restitution value="0.5" />
				<filter name="blueunit" />
				<mask bit4="0" />
				<group value="-1" />
			</circle>
			<box w="1" h="1">
				<position x="0" y="-2" />
				<density value="0" />
				<friction value="0.5" />
				<restitution value="0.5" />
				<filter name="blueunit" />
				<group value="-1" />
			</box>
		</body>
	</collidable>
	<drawlist>
		<triangle_strip>
			<vertex x="8" y="-8" />
			<vertex x="8" y="-5.5" />
			<vertex x="2.5" y="-8" />
			<vertex x="3" y="-3" />
			<vertex x="2" y="-6.5" />
			<vertex x="3" y="0.5" />
			<vertex x="1.5" y="-6.5" />
			<vertex x="1.5" y="2" />
			<vertex x="1" y="-8" />
			<vertex x="1" y="8" />
			<vertex x="0" y="-8" />
			<vertex x="-1" y="8" />
			<vertex x="-1" y="-8" />
			<vertex x="-1.5" y="2" />
			<vertex x="-1.5" y="-6.5" />
			<vertex x="-3" y="0.5" />
			<vertex x="-2" y="-6.5" />
			<vertex x="-3" y="-3" />
			<vertex x="-2.5" y="-8" />
			<vertex x="-8" y="-5.5" />
			<vertex x="-8" y="-8" />
		</triangle_strip>
	</drawlist>
	<renderable radius="8">
		<color r="0.4" g="0.5" b="1.0" a="1.0" />
		<calllist name="playership" />
	</renderable>
	<playercontroller>
		<scale turn="0.05"/>
	</playercontroller>
	<ship maxveloc="150" maxaccel="1200" minaccel="150" maxomega="20" />
	<damagable health="10" spawnondeath="playershipdeath" />
	<soundcue>
		<cue name="idle" sound="stargate-playershipidle" />
		<cue name="thrust" sound="stargate-playershipthrust" />
		<cue name="extralife" sound="stargate-extralife" />
	</soundcue>
	<team name="blue" />
</template>
