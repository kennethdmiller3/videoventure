<?xml version="1.0" ?>
<template name="enemymineshrapnel">
	<collidable>
		<body name="main">
			<fast value="1" />
			<circle radius="1">
				<friction value="1" />
				<restitution value="0" />
				<density value="1.0" />
				<filter name="redbullet" />
			</circle>
			<fixedrotation value="1" />
			<damping linear="1.0" />
		</body>
	</collidable>
	<renderable radius="2.5">
		<line_strip width="1">
			<color r="1.0" g="1.0" b="1.0" a="0.25" />
			<vertex x="0" y="0"/>
			<vertex x="0">
				<y>
					<key time="0" value="0"/>
					<key time="0.25" value="-45"/>
					<key time="0.5" value="-45"/>
					<key time="1" value="0"/>
				</y>
			</vertex>
		</line_strip>
		<drawlist>
			<pushmatrix>
				<color r="1.0" g="0.0" b="0.0" a="0.25" />
				<scale x="2.5" y="2.5" />
				<calllist name="diamond" />
			</pushmatrix>
			<pushmatrix>
				<color r="1.0" g="1.0" b="0.0" a="0.5" />
				<scale x="2.0" y="2.0" />
				<calllist name="diamond" />
			</pushmatrix>
			<pushmatrix>
				<color r="1.0" g="1.0" b="1.0" a="1.0" />
				<scale x="1.5" y="1.5" />
				<calllist name="diamond" />
			</pushmatrix>
		</drawlist>
	</renderable>
	<expire time="1" />
	<bullet damage="1" spawnondeath="enemybulletexplosion" />
	<cancelable switchoncancel="enemybulletexplosion" />
	<capturable resistance="1" spawnoncapture="friendmineshrapnel" />
</template>
