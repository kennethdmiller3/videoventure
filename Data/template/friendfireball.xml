<?xml version="1.0" ?>
<template name="friendfireball">
	<collidable>
		<body name="main">
			<fast value="1" />
			<circle radius="3">
				<friction value="1" />
				<restitution value="0" />
				<density value="1.0" />
				<category value="1" />
				<mask bit1="0" bit2="0" />
			</circle>
			<fixedrotation value="1" />
		</body>
	</collidable>
	<renderable radius="6" period="0.1">
		<drawlist>
			<pushmatrix>
				<color r="0.1" g="0.6" b="1.0" a="1.0" />
				<scale x="1.5" y="1.5" />
				<calllist name="circle16" />
			</pushmatrix>
			<pushmatrix>
				<color r="0.1" g="0.4" b="1.0" a="0.5" />
				<scale x="2.5" y="2.5" />
				<calllist name="circle16" />
			</pushmatrix>
			<pushmatrix>
				<color r="0.1" g="0.2" b="1.0" a="0.25" />
				<scale x="3.5" y="3.5" />
				<calllist name="circle16" />
			</pushmatrix>
		</drawlist>
		<rotate>
			<key time="0.0" angle="0" />
			<key time="0.1" angle="60" />
		</rotate>
		<pushmatrix>
			<color>
				<key time="0.0" r="0.1" g="0.2" b="1.0" a="0.5" />
				<key time="0.1" r="0.1" g="0.2" b="1.0" a="0.0" />
			</color>
			<repeat count="6">
				<pushmatrix>
					<translate>
						<key time="0.0" x="0" y="2" />
						<key time="0.1" x="0" y="6" />
					</translate>
					<scale x="0.5" y="1" />
					<calllist name="circle8" />
				</pushmatrix>
				<rotate angle="60" />
			</repeat>
		</pushmatrix>
		<rotate angle="15" />
		<pushmatrix>
			<color>
				<key time="0.00" r="0.1" g="0.2" b="1.0" a="0.25" />
				<key time="0.05" r="0.1" g="0.2" b="1.0" a="0.0" />
				<key time="0.05" r="0.1" g="0.2" b="1.0" a="0.5" />
				<key time="0.10" r="0.1" g="0.2" b="1.0" a="0.25" />
			</color>
			<repeat count="6">
				<pushmatrix>
					<translate>
						<key time="0.00" x="0" y="4" />
						<key time="0.05" x="0" y="6" />
						<key time="0.05" x="0" y="2" />
						<key time="0.10" x="0" y="4" />
					</translate>
					<scale x="0.5" y="1" />
					<calllist name="circle8" />
				</pushmatrix>
				<rotate angle="60" />
			</repeat>
		</pushmatrix>
	</renderable>
	<expire time="1" switchonexpire="friendbulletexplosion" />
	<bullet damage="1" spawnondeath="friendfireballexplosion" />
	<team name="blue" />
	<points value="100" />
	<spawner>
		<spawn name="friendfireballtrail" start="0" cycle="0.033333" track="0" />
	</spawner>
</template>
