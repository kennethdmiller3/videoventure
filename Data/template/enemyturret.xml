<?xml version="1.0" ?>
<template name="enemyturret" type="turret">
	<collidable>
		<body name="main">
			<circle radius="6">
				<density value="1" />
				<friction value="0.5" />
				<restitution value="0.5" />
				<filter name="redunit" />
			</circle>
		</body>
	</collidable>
	<renderable radius="6">
		<drawlist>
			<color r="0.0" g="0.0" b="0.0" a="0.5" />
			<pushmatrix>
				<scale x="6.5" y="6.5" />
				<calllist name="circle16" />
			</pushmatrix>
			<color r="1.0" g="0.2" b="0.1" a="1.0" />
			<pushmatrix>
				<scale x="6" y="6" />
				<calllist name="circle16" />
			</pushmatrix>
		</drawlist>
	</renderable>
	<team name="red" />
</template>
