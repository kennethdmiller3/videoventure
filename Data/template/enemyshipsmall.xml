<?xml version="1.0" ?>
<template name="enemyshipsmall">
	<collidable>
		<body name="main">
			<circle radius="6">
				<density value="1" />
				<friction value="0.5" />
				<restitution value="0.5" />
				<filter name="redunit" />
			</circle>
		</body>
	</collidable>
	<renderable radius="6" period="0.066667">
		<pushmatrix>
			<translate y="-6" />
			<calldynamiclist name="enemyengineflame" />
		</pushmatrix>
		<drawlist>
			<triangle_strip>
				<color r="0.0" g="0.0" b="0.0" a="0.5" />
				<vertex x="6.5" y="-6" />
				<vertex x="6.5" y="-3.25" />
				<vertex x="2.25" y="-6" />
				<vertex x="3.5" y="-1.75" />
				<vertex x="1.75" y="-5" />
				<vertex x="3.5" y="0.75" />
				<vertex x="1.75" y="-5" />
				<vertex x="2" y="2.25" />
				<vertex x="1.25" y="-6.5" />
				<vertex x="1.25" y="6.5" />
				<vertex x="0" y="-6.5" />
				<vertex x="-1.25" y="6.5" />
				<vertex x="-1.25" y="-6.5" />
				<vertex x="-2" y="2.25" />
				<vertex x="-1.75" y="-5" />
				<vertex x="-3.5" y="0.75" />
				<vertex x="-1.75" y="-5" />
				<vertex x="-3.5" y="-1.75" />
				<vertex x="-2.25" y="-6" />
				<vertex x="-6.5" y="-3.25" />
				<vertex x="-6.5" y="-6" />
			</triangle_strip>
		</drawlist>
		<color>
			<param>
				<min>
					<sub>
						<worldtime/>
						<lasthit/>
					</sub>
					<value value="0.05"/>
				</min>
			</param>
			<key time="0.000000" r="1.0" g="1.0" b="1.0" a="1.0" />
			<key time="0.033333" r="1.0" g="1.0" b="1.0" a="1.0" />
			<key time="0.050000" r="1.0" g="0.2" b="0.1" a="1.0" />
		</color>
		<drawlist>
			<triangle_strip>
				<vertex x="6" y="-5.5" />
				<vertex x="6" y="-3.5" />
				<vertex x="2.5" y="-5.5" />
				<vertex x="3" y="-2" />
				<vertex x="2" y="-4.5" />
				<vertex x="3" y="0.5" />
				<vertex x="1.5" y="-4.5" />
				<vertex x="1.5" y="2" />
				<vertex x="1" y="-6" />
				<vertex x="1" y="6" />
				<vertex x="0" y="-6" />
				<vertex x="-1" y="6" />
				<vertex x="-1" y="-6" />
				<vertex x="-1.5" y="2" />
				<vertex x="-1.5" y="-4.5" />
				<vertex x="-3" y="0.5" />
				<vertex x="-2" y="-4.5" />
				<vertex x="-3" y="-2" />
				<vertex x="-2.5" y="-5.5" />
				<vertex x="-6" y="-3.5" />
				<vertex x="-6" y="-5.5" />
			</triangle_strip>
		</drawlist>
	</renderable>
	<aimer>
		<drift strength="0.5"/>
		<wander turn="0.5" turnrate="0.25" />
		<target range="256" filter="redbullet" />
		<aim strength="0.5" leading="0" />
		<pursue strength="1" leading="0" />
		<fire channel="1" range="128" angle="30" />
		<evade strength="1" />
		<close range="64" />
		<edge strength="0.015625" distance="64" />
	</aimer>
	<ship forwardveloc="75" neutralveloc="0" reverseveloc="-30" strafeveloc="30" maxaccel="120" minaccel="60" maxomega="1.5" />
	<damagable health="3" spawnondeath="enemyexplosionsmall" />
	<link name="gun" updateangle="0">
		<collidable>
			<body name="main">
				<circle radius="2">
					<density value="0.5"/>
					<friction value="0.5"/>
					<restitution value="0.5"/>
					<filter name="inert"/>
				</circle>
			</body>
		</collidable>
		<aimer>
			<target range="256" filter="redbullet" />
			<aim strength="1" leading="0" />
			<fire channel="1" range="128" angle="15" />
		</aimer>
		<ship maxveloc="0" maxaccel="0" minaccel="0" maxomega="3"/>
		<weapon>
			<shot delay="0.833333" />
			<action>
				<wait>
					<mul>
						<frac>
							<mul>
								<worldtime/>
								<value value="-2.4"/>
							</mul>
						</frac>
						<value value="0.416667"/>
					</mul>
				</wait>
				<ordnance name="enemypulsebullet">
					<velocity x="0" y="120" />
				</ordnance>
				<cue name="fire"/>
				<wait value="0.208333"/>
				<ordnance name="enemypulsebullet">
					<velocity x="0" y="120" />
				</ordnance>
				<cue name="fire"/>
				<wait value="0.208333"/>
				<ordnance name="enemypulsebullet">
					<velocity x="0" y="120" />
				</ordnance>
				<cue name="fire"/>
			</action>
		</weapon>
		<soundcue>
			<cue name="fire" sound="stargate-landerfire" />
		</soundcue>
	</link>
	<team name="red" />
	<points value="200" />
</template>
