	<template name="playerbullet">
		<collidable>
			<body name="main">
				<fast value="1" />
				<circle radius="1">
					<friction value="1" />
					<restitution value="0" />
					<density value="1.0" />
					<filter name="bluebullet" />
				</circle>
				<fixedrotation value="1" />
			</body>
		</collidable>
		<renderable radius="6">
			<drawlist>
				<color r="0.0" g="0.5" b="1.0" a="0.25" />
				<pushmatrix>
					<scale x="1.5" y="6" />
					<calllist name="circle8" />
				</pushmatrix>
				<color r="0.4" g="0.7" b="1.0" a="0.5" />
				<pushmatrix>
					<scale x="1.0" y="5" />
					<calllist name="circle8" />
				</pushmatrix>
				<color r="1.0" g="1.0" b="1.0" a="1.0" />
				<pushmatrix>
					<scale x="0.5" y="4" />
					<calllist name="circle8" />
				</pushmatrix>
			</drawlist>
		</renderable>
		<expire time="1" />
		<bullet damage="1" spawnondeath="playerbulletexplosion" />
	</template>
	<!-- player ship weapon -->
	<template name="playershipweapon">
		<weapon>
