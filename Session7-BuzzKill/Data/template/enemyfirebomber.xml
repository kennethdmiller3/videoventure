<?xml version="1.0" ?>
<template name="enemyfirebomber">
	<collidable>
		<body name="main">
			<circle radius="6">
				<density value="1" />
				<friction value="0.5" />
				<restitution value="0.5" />
				<filter name="redunit" />
			</circle>
		</body>
	</collidable>
	<renderable radius="6" period="1.0">
		<pushmatrix>
			<rotate>
				<key time="0.0" angle="0" />
				<key time="1.0" angle="2700" />
			</rotate>
			<color>
				<key time="0.0" r="1.0" g="0.8" b="0.1" a="1.0" />
				<key time="0.5" r="1.0" g="0.2" b="0.1" a="1.0" />
				<key time="1.0" r="1.0" g="0.8" b="0.1" a="1.0" />
			</color>
			<scale x="2.5" y="2.5" />
			<calllist name="bowtie" />
			<scale x="1.5" y="1.5" />
			<color r="1.0" g="1.0" b="1.0" a="1.0" />
			<calllist name="bowtie" />
			<color>
				<key time="0.0" r="1.0" g="0.2" b="0.1" a="1.0" />
				<key time="0.5" r="1.0" g="0.8" b="0.1" a="1.0" />
				<key time="1.0" r="1.0" g="0.2" b="0.1" a="1.0" />
			</color>
			<scale x="1.5" y="1.5" />
			<calllist name="bowtie" />
		</pushmatrix>
	</renderable>
	<!--aimer period="0.1" range="128" attack="128" leading="180" drift="0.25" wanderturn="0.25" wanderturnrate="0.25" pursue="0" evade="1" close="64" category="3" bit3="0" bit4="0" /-->
	<aimer>
		<drift strength="0.25" />
		<wander turn="0.25" turnrate="0.25" />
		<target period="0.1" range="128" category="3" bit3="0" bit4="0" />
		<aim strength="1" leading="180" />
		<fire channel="1" range="128" />
		<evade strength="1" />
		<close range="64" />
	</aimer>
	<ship maxveloc="120" maxaccel="360" minaccel="10" maxomega="2" />
	<damagable health="3" spawnondeath="enemyexplosion" />
	<weapon>
		<velocity x="0" y="180" />
		<ordnance name="enemyfireball" />
		<shot delay="0.3" track="1" />
	</weapon>
	<soundcue>
		<cue name="fire" sound="stargate-firebomberfire" />
	</soundcue>
	<team name="red" />
	<points value="250" />
	<capturable resistance="3" spawnoncapture="friendfirebomber" />
</template>
