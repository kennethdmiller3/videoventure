<?xml version="1.0" ?>
<template name="friendmineshrapnel">
	<trace filter="bluebullet">
		<damping linear="1.0" />
	</trace>
	<renderable radius="2.5">
		<drawlist>
			<color r="0.0" g="0.5" b="1.0" a="0.25" />
			<pushmatrix>
				<scale x="2.5" y="2.5" />
				<calllist name="diamond" />
			</pushmatrix>
			<color r="0.4" g="0.7" b="1.0" a="0.5" />
			<pushmatrix>
				<scale x="2.0" y="2.0" />
				<calllist name="diamond" />
			</pushmatrix>
			<color r="1.0" g="1.0" b="1.0" a="1.0" />
			<pushmatrix>
				<scale x="1.5" y="1.5" />
				<calllist name="diamond" />
			</pushmatrix>
		</drawlist>
	</renderable>
	<expire time="1" />
	<bullet damage="1" spawnondeath="friendbulletexplosion" />
</template>
