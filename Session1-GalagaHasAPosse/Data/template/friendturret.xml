<?xml version="1.0" ?>
<template name="friendturret" type="turret">
	<collidable>
		<body name="main">
			<circle radius="6">
				<density value="1" />
				<friction value="0.5" />
				<restitution value="0.5" />
				<filter name="blueunit" />
			</circle>
		</body>
	</collidable>
	<renderable radius="6">
		<drawlist>
			<color r="0.1" g="0.2" b="1.0" a="1.0" />
			<pushmatrix>
				<scale x="6" y="6" />
				<calllist name="circle16" />
			</pushmatrix>
		</drawlist>
	</renderable>
	<team name="blue" />
</template>
