<?xml version="1.0" ?>
<template name="enemywarrior">
	<collidable>
		<body name="main">
			<circle radius="8">
				<density value="1" />
				<friction value="0.5" />
				<restitution value="0.5" />
				<filter name="redunit" />
			</circle>
			<fixedrotation value="1" />
		</body>
	</collidable>
	<renderable radius="9">
		<drawlist>
			<color r="0.3" g="0.3" b="0.3" a="1.0" />
			<triangle_strip>
				<vertex x="8.5" y="2.5" />
				<vertex x="2.5" y="8.5" />
				<vertex x="8.5" y="2" />
				<vertex x="2" y="8.5" />
				<vertex x="7.5" y="1.5" />
				<vertex x="1.5" y="7.5" />
				<vertex x="7.5" y="-2" />
				<vertex x="-1.5" y="7.5" />
				<vertex x="8.5" y="-1.5" />
				<vertex x="-2" y="8.5" />
				<vertex x="8.5" y="-2.5" />
				<vertex x="-2.5" y="8.5" />
				<vertex x="2.5" y="-8.5" />
				<vertex x="-8.5" y="2.5" />
				<vertex x="2" y="-8.5" />
				<vertex x="-8.5" y="2" />
				<vertex x="1.5" y="-7.5" />
				<vertex x="-7.5" y="1.5" />
				<vertex x="-1.5" y="-7.5" />
				<vertex x="-7.5" y="-1.5" />
				<vertex x="-2" y="-8.5" />
				<vertex x="-8.5" y="-2" />
				<vertex x="-2.5" y="-8.5" />
				<vertex x="-8.5" y="-2.5" />
			</triangle_strip>
			<pushmatrix>
				<repeat count="4">
					<color r="0.45" g="0.15" b="0.35" a="1.0" />
					<triangle_strip>
						<vertex x="3.5" y="3.5" />
						<vertex x="2.5" y="3.5" />
						<vertex x="3.5" y="2.5" />
						<vertex x="2.5" y="2.5" />
					</triangle_strip>
					<triangle_strip>
						<vertex x="5.5" y="5.5" />
						<vertex x="2.5" y="5.5" />
						<vertex x="5.5" y="2.5" />
						<vertex x="2.5" y="4.5" />
						<vertex x="4.5" y="2.5" />
					</triangle_strip>
					<triangle_strip>
						<vertex x="7.5" y="0.5" />
						<vertex x="7.5" y="-0.5" />
						<vertex x="6.5" y="1.5" />
						<vertex x="6.5" y="-1.5" />
						<vertex x="4.5" y="1.5" />
						<vertex x="4.5" y="-1.5" />
						<vertex x="4.5" y="0.5" />
						<vertex x="4.5" y="-0.5" />
						<vertex x="3.5" y="0.5" />
						<vertex x="3.5" y="-0.5" />
						<vertex x="3.5" y="1.5" />
						<vertex x="3.5" y="-1.5" />
						<vertex x="2.5" y="1.5" />
						<vertex x="2.5" y="-1.5" />
					</triangle_strip>
					<color r="1.0" g="0.2" b="0.1" a="1.0" />
					<triangle_strip>
						<vertex x="5.5" y="3.5" />
						<vertex x="5.5" y="4.5" />
						<vertex x="4.5" y="3.5" />
						<vertex x="4.5" y="4.5" />
						<vertex x="3.5" y="3.5" />
						<vertex x="4.5" y="4.5" />
						<vertex x="3.5" y="4.5" />
						<vertex x="3.5" y="5.5" />
						<vertex x="4.5" y="5.5" />
					</triangle_strip>
					<triangle_strip>
						<vertex x="6.5" y="0.5" />
						<vertex x="6.5" y="-0.5" />
						<vertex x="5.5" y="0.5" />
						<vertex x="5.5" y="-0.5" />
					</triangle_strip>
					<rotate angle="90" />
				</repeat>
			</pushmatrix>
		</drawlist>
	</renderable>
	<link name="turret" secondary="enemywarriorturret" updateangle="0" />
	<!--aimer range="256" attack="0" leading="0" wanderside="0.5" wandersiderate="0.125" wanderfront="0.5" wanderfrontrate="0.125" evade="1" close="64" filter="redbullet" /-->
	<aimer>
		<wander side="0.5" siderate="0.125" front="0.5" frontrate="0.125" />
		<target range="256" filter="redbullet" />
		<pursue strength="1" leading="0" />
		<evade strength="1" />
		<close range="64" />
	</aimer>
	<ship maxveloc="60" maxaccel="120" minaccel="10" maxomega="0.0" />
	<damagable health="15" spawnondeath="enemyexplosion" />
	<team name="red" />
	<points value="500" />
	<capturable resistance="15" spawnoncapture="friendwarrior" />
</template>
